{
  "name": "jjuivscode",
  "displayName": "jjuivscode - Jujutsu VCS TUI",
  "description": "Open the Jujutsu TUI jjui inside VSCode terminal",
  "keywords": [
    "jujutsu",
    "jjui",
    "tui",
    "vscode",
    "extension"
  ],
  "version": "0.0.5",
  "icon": "jjuivscodeloge.png",
  "publisher":"Yetisunny",
  "repository": {
    "type": "git",
    "url": "https://github.com/yetisunny/jjui-vscode"
},
  "engines": {
    "vscode": "^1.103.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "jjui-vscode.toggle",
        "title": "Toggle TUI",
        "category": "jjui"
      }
    ],
    "configuration": {
      "title": "jjui-vscode",
      "properties": {
        "jjui-vscode.jjuiPath": {
          "type": "string",
          "default": "",
          "description": "Custom path to jjui binary"
        },
        "jjui-vscode.autoMaximizeWindow": {
          "type": "boolean",
          "default": true
        },
        "jjui-vscode.panels.sidebar": {
          "type": "string",
          "enum": [
            "keep",
            "hide",
            "hideRestore"
          ],
          "default": "hideRestore"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.103.0",
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^8.31.1",
    "@typescript-eslint/parser": "^8.31.1",
    "eslint": "^9.25.1",
    "typescript": "^5.8.3",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.5.2"
  }
}
